{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "MCP (Model Control Plane) server that integrates with ChatGPT through Railway deployment with WebContainers",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "node scripts/validate-env.js",
    "build": "yarn build:server && yarn build:next",
    "build:server": "tsc -p tsconfig.server.json",
    "build:next": "next build frontend",
    "build:all": "concurrently \"yarn build:server\" \"yarn build:next\"",
    "start": "node dist/server.js",
    "dev": "nodemon --exec tsx src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "verify": "node scripts/verify-build.js || echo 'Verification skipped'",
    "railway:validate": "node scripts/railway-validation/validator.cjs",
    "railway:validate-env": "node scripts/railway-validation/env-validator.cjs",
    "railway:validate-auth": "node scripts/railway-validation/auth-validator.cjs",
    "railway:report": "node scripts/railway-validation/generate-report.cjs",
    "railway:check-all": "npm run railway:validate && npm run railway:validate-env && npm run railway:validate-auth",
    "docs:check-links": "npx linkinator docs/ --recurse --timeout 10000",
    "setup:dev": "./scripts/setup-dev-environment.sh"
  },
  "keywords": [
    "mcp",
    "webcontainer",
    "chatgpt",
    "railway",
    "development-environment"
  ],
  "author": "Arcane-Fly",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@stackblitz/sdk": "^1.9.0",
    "@types/archiver": "^6.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/dockerode": "^3.3.42",
    "@types/react": "^18.3.11",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-dom": "^18.3.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@webcontainer/api": "^1.1.9",
    "archiver": "^7.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dockerode": "^4.0.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.12",
    "helmet": "^7.1.0",
    "immer": "^10.1.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "next": "^14.2.3",
    "opossum": "^9.0.0",
    "playwright": "^1.54.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.14.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "axios": "^1.11.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
