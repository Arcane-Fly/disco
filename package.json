{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "MCP (Model Control Plane) server that integrates with ChatGPT through Railway deployment with WebContainers",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "typecheck": "tsc -p tsconfig.server.json --noEmit",
    "prebuild": "echo 'TypeScript build - ignoring non-critical errors for Railway deployment compatibility'",
    "build": "yarn build:server && yarn build:next",
    "build:server": "tsc -p tsconfig.server.json",
    "build:next": "next build frontend",
    "build:all": "concurrently \"yarn build:server\" \"yarn build:frontend\"",
    "start": "node dist/server.js",
    "dev": "nodemon --exec tsx src/server.ts",
    "test": "jest",
    "test:contracts": "jest test/contract-validation.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "quality": "yarn lint:fix && yarn format",
    "clean": "rimraf dist",
    "verify": "node scripts/verify-build.js || echo 'Verification skipped'",
    "railway:validate": "node scripts/railway-validation/validator.cjs",
    "railway:validate-env": "node scripts/railway-validation/env-validator.cjs",
    "railway:validate-auth": "node scripts/railway-validation/auth-validator.cjs",
    "railway:report": "node scripts/railway-validation/generate-report.cjs",
    "railway:check-all": "yarn railway:validate && yarn railway:validate-env && yarn railway:validate-auth",
    "docs:check-links": "yarn dlx linkinator docs/ --recurse --timeout 10000",
    "setup:dev": "./scripts/setup-dev-environment.sh",
    "constraints": "yarn constraints check",
    "constraints:fix": "yarn constraints --fix"
  },
  "keywords": [
    "mcp",
    "webcontainer",
    "chatgpt",
    "railway",
    "development-environment"
  ],
  "author": "Arcane-Fly",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@stackblitz/sdk": "^1.11.0",
    "@types/archiver": "^6.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/dockerode": "^3.3.44",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@webcontainer/api": "^1.6.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "archiver": "^7.0.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
