{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://disco-mcp.up.railway.app/contracts/browserbase/navigate.response.json",
  "title": "browserbase.navigate.response",
  "description": "Response schema for browser navigation",
  "type": "object",
  "additionalProperties": false,
  "required": ["url", "title", "statusCode"],
  "properties": {
    "url": {
      "type": "string",
      "format": "uri",
      "description": "Final URL after navigation (may differ due to redirects)"
    },
    "title": {
      "type": "string",
      "description": "Page title"
    },
    "statusCode": {
      "type": "integer",
      "minimum": 100,
      "maximum": 599,
      "description": "HTTP status code"
    },
    "loadTime": {
      "type": "number",
      "minimum": 0,
      "description": "Page load time in milliseconds"
    },
    "screenshot": {
      "type": "string",
      "description": "Base64-encoded screenshot (if requested)"
    }
  }
}
