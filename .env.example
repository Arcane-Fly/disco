# Environment variables for MCP Server

# Server Configuration
PORT=3000
NODE_ENV=development

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
ALLOWED_ORIGINS=https://chat.openai.com,https://chatgpt.com,http://localhost:3000

# WebContainer Integration
WEBCONTAINER_API_KEY=your-webcontainer-api-key
WEBCONTAINER_CLIENT_ID=your-webcontainer-client-id

# WebSocket Configuration (Railway auto-generated in production)
WEBSOCKET_URL=ws://localhost:3000/socket.io

# GitHub Integration
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_USERNAME=your-github-username
GITHUB_USEREMAIL=your-github-email
# OAuth callback URL - configure this in your GitHub OAuth app settings
# Default: /api/v1/auth/github/callback (handles OAuth flow and redirects to frontend)
AUTH_CALLBACK_URL=https://your-domain.up.railway.app/api/v1/auth/github/callback

# Database Configuration (Postgres)
DATABASE_URL=postgresql://user:password@localhost:5432/disco
DATABASE_PUBLIC_URL=postgresql://user:password@localhost:5432/disco
PGHOST=localhost
PGPORT=5432
PGDATABASE=disco
PGUSER=postgres
POSTGRES_DB=disco
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-postgres-password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USER=default
REDIS_PASSWORD=your-redis-password
REDISHOST=localhost
REDISPORT=6379
REDISUSER=default
REDISPASSWORD=your-redis-password

# AI Provider API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GEMINI_API_KEY=your-gemini-api-key
GOOGLE_API_KEY=your-google-api-key
PERPLEXITY_API_KEY=your-perplexity-api-key
QWEN_ACCESS_KEY_SECRET=your-qwen-access-key

# API Keys (Comma-separated list of valid API keys)
VALID_API_KEYS=test-key-1,test-key-2

# Container Limits
MAX_CONTAINERS=50
CONTAINER_TIMEOUT_MINUTES=30
POOL_SIZE=5

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Logging
LOG_LEVEL=info

# Data Directory
DATA_DIR=app/data

# Production Configuration
NPM_CONFIG_PRODUCTION=false

