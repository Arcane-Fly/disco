    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "nx": "^21.6.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.14",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.11"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "yarn@4.9.2",
  "types": "./src/server.d.ts",
  "module": "./src/server.js"
}