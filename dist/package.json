{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "MCP (Model Control Plane) server that integrates with ChatGPT through Railway deployment with WebContainers",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "// Build commands with Nx": "",
    "typecheck": "nx run-many --target=typecheck --all",
    "prebuild": "echo 'TypeScript build with Nx caching - ignoring non-critical errors for Railway deployment compatibility'",
    "build": "nx run-many --target=build --all --parallel=2",
    "build:server": "nx build server",
    "build:next": "nx build frontend",
    "build:frontend": "nx build frontend",
    "build:all": "nx run-many --target=build --all --parallel=2",
    "// Development commands": "",
    "start": "node --expose-gc dist/src/server.js",
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:frontend": "nx serve frontend",
    "// Testing commands": "",
    "test": "nx run-many --target=test --all",
    "test:server": "nx test server",
    "test:contracts": "jest test/contract-validation.test.ts",
    "test:affected": "nx affected --target=test",
    "// Linting commands": "",
    "lint": "nx run-many --target=lint --all",
    "lint:server": "nx lint server",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected --target=lint",
    "// Formatting commands": "",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{json,md}\"",
    "quality": "yarn lint:fix && yarn format",
    "// Nx utilities": "",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "reset": "nx reset",
    "// Maintenance commands": "",
    "clean": "rimraf dist && nx reset",
    "verify": "node scripts/verify-build.js || echo 'Verification skipped'",
    "// Railway validation": "",
    "railway:validate": "node scripts/railway-validation/validator.cjs",
    "railway:validate-env": "node scripts/railway-validation/env-validator.cjs",
    "railway:validate-auth": "node scripts/railway-validation/auth-validator.cjs",
    "railway:report": "node scripts/railway-validation/generate-report.cjs",
    "railway:check-all": "yarn railway:validate && yarn railway:validate-env && yarn railway:validate-auth",
    "// Callback URL validation": "",
    "validate:callbacks": "tsx scripts/validate-callback-urls.ts",
    "// Documentation": "",
    "docs:check-links": "yarn dlx linkinator docs/ --recurse --timeout 10000",
    "setup:dev": "./scripts/setup-dev-environment.sh",
    "constraints": "yarn constraints check",
    "constraints:fix": "yarn constraints --fix",
    "// Storybook": "",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "keywords": [
    "mcp",
    "webcontainer",
    "chatgpt",
    "railway",
    "development-environment"
  ],
  "author": "Arcane-Fly",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@types/archiver": "^6.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/dockerode": "^3.3.44",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@webcontainer/api": "^1.6.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "archiver": "^7.0.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "immer": "^10.1.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "opossum": "^9.0.0",
    "playwright": "^1.55.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "recharts": "^3.2.1",
    "redis": "^5.8.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
